{
  "name": "doeet-mapper",
  "version": "1.0.1",
  "description": "remap vscode browser urls to vscode insiders",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/mcansh/doeet-mapper",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
  "scripts": {
    "prebuild": "del dist",
    "build": "tsc",
    "postbuild": "cp src/{icon-{256,512,1024}.png,manifest.json} dist",
    "prepare": "npm run build",
    "format": "prettier --ignore-path .gitignore --write ."
  },
  "devDependencies": {
    "del-cli": "4.0.1",
    "prettier": "2.5.1",
    "typescript": "4.3.5"
  }
}
